{
  "type": "module",
  "name": "swift-cloud",
  "description": "Taylor Swift songs query api.",
  "version": "1.0.0",
  "main": "server.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && c8 tap --ts \"test/**/*.test.ts\"",
    "start": "npm run build:ts && Node dist/server.js",
    "build:ts": "tsc",
    "lint": "eslint .",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start-old": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "dev:start": "node --watch dist/server.js"
  },
  "keywords": [],
  "author": "Phuong Nguyen",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/mongodb": "^8.0.0",
    "@fastify/sensible": "^5.0.0",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.8.0",
    "fastify-plugin": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "@types/tap": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "c8": "^7.13.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "fastify-tsconfig": "^1.0.1",
    "prettier": "3.0.3",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.9.5"
  },
  "tap": {
    "node-arg": [
      "--no-warnings",
      "--experimental-loader",
      "ts-node/esm"
    ],
    "coverage": false
  }
}
